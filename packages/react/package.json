{
  "name": "@chunk-db/react",
  "version": "0.0.0",
  "description": "React binding for ChunkDB",
  "author": "Arswarog <arswarog@yandex.ru>",
  "homepage": "",
  "license": "MIT",
  "source": "src/index.ts",
  "main": "dist/index.js",
  "module": "dist/foo.module.js",
  "unpkg": "dist/foo.umd.js",
  "files": [
    "src"
  ],
  "scripts": {
    "build": "yarn run build:package",
    "build:package": "microbundle",
    "dev": "microbundle --no-compress watch",
    "test": "jest -c ../../jest.config.js",
    "docs": "npm run docs:clean && npm run docs:generate",
    "docs:clean": "rimraf _docs",
    "docs:generate": "typedoc",
    "predocs:export": "mkdir -p ../../_docs/react",
    "docs:export": "cp -r _docs/* ../../_docs/react"
  },
  "devDependencies": {
    "@chunk-db/core": "^0.0.0",
    "@types/react": "^17.0.5",
    "microbundle": "^0.12.4"
  },
  "peerDependencies": {
    "@chunk-db/core": "^0.0.0",
    "react": "^17.0.2"
  }
}
